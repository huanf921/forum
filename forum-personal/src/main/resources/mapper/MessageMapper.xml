<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.vs.forum.mapper.MessageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="Message">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="type" property="type" />
        <result column="subscribe_id" property="subscribeId" />
        <result column="post_id" property="postId" />
        <result column="reply_user_id" property="replyUserId" />
        <result column="reply_post_id" property="replyPostId" />
        <result column="reply_comment_id" property="replyCommentId" />
        <result column="reply_reply_id" property="replyReplyId" />
        <result column="to_subscribe_id" property="toSubscribeId" />
        <result column="time" property="time" />
        <result column="readed" property="readed" />
    </resultMap>

</mapper>
